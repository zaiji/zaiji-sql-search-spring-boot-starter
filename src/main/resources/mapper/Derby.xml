<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zaiji.starter.sql.dao.DerbyDao">
    <resultMap id="startLogResultMap" type="com.zaiji.starter.sql.entity.StartLog">
        <result column="START_TIME" property="startTime" jdbcType="BIGINT"></result>
        <result column="IP_PORT" property="ipPort" jdbcType="VARCHAR"></result>
        <result column="SERVICE_NAME" property="serviceName" jdbcType="VARCHAR"></result>
    </resultMap>
    <resultMap id="receiverLogResultMap" type="com.zaiji.starter.sql.entity.ReceiverLog">
        <result column="RECEIVER_TIME" property="receiverTime" jdbcType="BIGINT"></result>
        <result column="PROCESSING_TIME" property="processingTime" jdbcType="BIGINT"></result>
        <result column="COMPLETION_TIME" property="completionTime" jdbcType="BIGINT"></result>
        <result column="RECEIVER_STATUS" property="receiverStatus" jdbcType="VARCHAR"></result>
        <result column="HANDLE_STATUS" property="handleStatus" jdbcType="VARCHAR"></result>
        <result column="DATA_SOURCE_TYPE" property="dataSourceType" jdbcType="VARCHAR"></result>
        <result column="DATA_SOURCE_INFO_JSON_TEXT" property="dataSourceInfoJsonText" jdbcType="VARCHAR"></result>
        <result column="RECEIVER_DATA_CONTEXT" property="receiverDataContext" jdbcType="VARCHAR"></result>
        <result column="HANDLE_INFO" property="handleInfo" jdbcType="VARCHAR"></result>
        <result column="OTHER_INFO" property="otherInfo" jdbcType="VARCHAR"></result>
    </resultMap>

    <select id="getReceiverLogs" resultMap="receiverLogResultMap">
        select receiver_time,
               processing_time,
               completion_time,
               receiver_status,
               handle_status,
               data_source_type,
               data_source_info_json_text,
               receiver_data_context,
               handle_info,
               other_info
        from wisvision_receiver_log
    </select>

</mapper>